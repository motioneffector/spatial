{
  "name": "@motioneffector/spatial",
  "version": "0.1.0",
  "description": "Directional graph for room-based spatial navigation",
  "author": "motioneffector",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motioneffector/spatial"
  },
  "homepage": "https://github.com/motioneffector/spatial#readme",
  "bugs": {
    "url": "https://github.com/motioneffector/spatial/issues"
  },
  "keywords": [
    "spatial",
    "navigation",
    "graph",
    "game-development",
    "map",
    "pathfinding",
    "room-based",
    "directional",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run build:manual",
    "build:manual": "node scripts/build-manual.cjs",
    "test": "vitest",
    "test:run": "vitest run",
    "fuzz:thorough": "FUZZ_THOROUGH=1 vitest run src/fuzz.test.ts --testTimeout=120000",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test:run && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "marked": "^12.0.2",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
